{
  "name": "failean-mainserver",
  "version": "4.3.3",
  "scripts": {
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "clean": "rm -rf node_modules package-lock.json dist",
    "clean:p": "rm -rf node_modules package-lock.json",
    "preupdatesimple": "npx npm-check-updates -u",
    "updatesimple": "npm i",
    "preupdate": "npm run clean",
    "update": "npm run updatesimple",
    "prebuild": "npm run version:patch",
    "build": "npx ts-node build.ts",
    "lint": "eslint . --ext .ts",
    "start": "npm run udev",
    "preprod": "npm run update",
    "prod": "npm run build",
    "d:b": "VERSION=$(node -p \"require('./package.json').version\") && docker build --platform amd64 -t 988253048728.dkr.ecr.us-east-1.amazonaws.com/mainserver:$VERSION .",
    "dev": "nodemon",
    "preudev": "npm run update",
    "udev": "npm run dev",
    "pred:b": "rm -f .npmrc"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node ./src"
  },
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@failean/shared-types": "^2.9.159",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/fs-extra": "^11.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.5.5",
    "@types/zxcvbn": "^4.4.1",
    "apollo-server-core": "^3.12.0",
    "cors": "^2.8.5",
    "fs-extra": "^11.1.1",
    "graphql": "^16.7.1",
    "jet-logger": "^1.3.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0"
  },
  "dependencies": {
    "@bull-board/api": "^5.7.1",
    "@bull-board/express": "^5.7.1",
    "@failean/mongo-models": "^1.6.121",
    "@sendgrid/mail": "^7.7.0",
    "apollo-server-express": "^3.12.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express-basic-auth": "^1.2.1",
    "gpt-3-encoder": "^1.1.4",
    "graphql-redis-subscriptions": "^2.6.0",
    "graphql-ws": "^5.14.0",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.4.1",
    "openai": "^3.3.0",
    "ws": "^8.13.0",
    "zxcvbn": "^4.4.2"
  }
}
