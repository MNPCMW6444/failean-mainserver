{
  "name": "failean-mainserver",
  "version": "8.0.0",
  "private": true,
  "scripts": {
    "v..": "npm version patch --no-git-tag-version",
    "v.": "npm version minor --no-git-tag-version",
    "v": "npm version major --no-git-tag-version",
    "clean:all": "rm -rf node_modules dist package-lock.json",
    "clean:prod": "rm -rf node_modules package-lock.json",
    "update": "npx npm-check-updates -u && npm i",
    "build": "tsc",
    "start": "npm run clean:all && npm run dev:fullfast",
    "prod": "npm run update && npm run build",
    "dev:fullclean": "npm run clean:all && npm run dev:fullfast",
    "dev:fullfast": "npm run update && npm run dev:fast",
    "dev:fast": "nodemon",
    "dev": "npm run dev:fast",
    "lint": "eslint . --ext .ts"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node ./src"
  },
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "newrelic": "latest",
    "@bull-board/api": "^5.8.3",
    "@bull-board/express": "^5.8.3",
    "@failean/mongo-models": "^1.6.129",
    "@failean/oc-server-axiosinstance": "^5.5.67",
    "@sendgrid/mail": "^7.7.0",
    "apollo-server-express": "^3.12.0",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "bull": "^4.11.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express-basic-auth": "^1.2.1",
    "gpt-3-encoder": "^1.1.4",
    "graphql-redis-subscriptions": "^2.6.0",
    "graphql-ws": "^5.14.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.2",
    "nodemon": "^3.0.1",
    "openai": "^4.7.1",
    "ws": "^8.14.1",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@failean/shared-types": "^2.9.176",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.4",
    "@types/fs-extra": "^11.0.2",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/uuid": "^9.0.4",
    "@types/ws": "^8.5.5",
    "@types/zxcvbn": "^4.4.2",
    "apollo-server-core": "^3.12.0",
    "cors": "^2.8.5",
    "fs-extra": "^11.1.1",
    "graphql": "^16.8.0",
    "jet-logger": "^1.3.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.1"
  }
}
