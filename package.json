{
  "name": "failer-server-main",
  "version": "0.0.35",
  "scripts": {
    "build": "npx ts-node build.ts",
    "update": "npx npm-check-updates -u",
    "lint": "eslint . --ext .ts",
    "prestart": "npm run build",
    "start": "node ./dist --env=production",
    "predev": "npm install",
    "dev": "nodemon",
    "prebuild": "npm run version:patch",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node ./src"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/uuid": "^9.0.1",
    "@types/zxcvbn": "^4.4.1",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "jet-logger": "^1.3.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.2.1",
    "nodemon": "^2.0.22",
    "openai": "^3.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "zxcvbn": "^4.4.2"
  }
}
